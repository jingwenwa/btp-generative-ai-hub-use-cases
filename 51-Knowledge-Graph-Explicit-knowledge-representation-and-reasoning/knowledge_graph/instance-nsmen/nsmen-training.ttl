@prefix :      <http://www.semanticweb.org/ontologies/2025/smart-technical-advisory-rdf-test/> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .

:TrainingScenario rdf:type owl:Class ;
    rdfs:label "NSmen Training Scenario" ;
    rdfs:comment "Various scenarios used in NSmen training simulations" .

:UrbanOps rdf:type owl:NamedIndividual , :TrainingScenario ;
    rdfs:comment "Urban operations simulation for NSmen training" ;
    :hasName "Urban Ops" .

:MedicalSupport rdf:type owl:NamedIndividual , :TrainingScenario ;
    rdfs:comment "Medical support and evacuation exercise" ;
    :hasName "Medical Support" .

:CyberDefense rdf:type owl:NamedIndividual , :TrainingScenario ;
    rdfs:comment "Cyber defense and response training" ;
    :hasName "Cyber Defense" .
